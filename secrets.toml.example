# Example secrets.toml file for Streamlit Community Cloud
# Rename this file to .streamlit/secrets.toml for local development
# For Streamlit Community Cloud, add these secrets in the app settings

[edsl]
api_key = "your-edsl-api-key-here"

# Add your provider API keys as needed
[edsl.openai]
api_key = "your-openai-api-key-here"

[edsl.anthropic]
api_key = "your-anthropic-api-key-here"

[edsl.google]
api_key = "your-google-api-key-here"

